<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ling.stums.mapper.ClazzMapper">

    <update id="updatePopulation">
        update clazz set population = population +1 where cid=#{cid}
    </update>
</mapper>
